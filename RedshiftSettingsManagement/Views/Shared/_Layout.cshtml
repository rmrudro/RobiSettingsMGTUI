    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />


    <link href="~/css/bootstrap/bootstrap431min.css" rel="stylesheet" />

    <link href="~/css/bootstrap/bootstrap-datetimepicker.min.css" rel="stylesheet" />

    <meta name="viewport" content="width=device-width, initial-scale=1">


    <link href="~/css/toast/jquery_toast.min.css" rel="stylesheet" />


    <script src="~/js/jquery-3.3.1.min.js"></script>


    <script src="~/js/bootstrap4_0_0_min/proper_1_11_0_min.js"></script>

    <script src="~/js/bootstrap4_0_0_min/bootstrap_min_4_0_0.js"></script>

