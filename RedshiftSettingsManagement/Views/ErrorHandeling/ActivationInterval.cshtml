
@{
    ViewData["Title"] = "ActivationInterval";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/css/site.css" rel="stylesheet" />
<link href="~/css/Delete_Confirmation/bootstrap-confirm-delete.css" rel="stylesheet" />
<link type="text/css" href="~/css/DataTable/DataTable_1_10_19.css" rel="stylesheet" />
<style>

    #load {
        width: 100%;
        height: 100%;
        position: fixed;
        z-index: 9999;
        background: url('/images/loader/lg.comet-spinner.gif') no-repeat center center rgba(0,0,0,0.25);
        /*background: url('../../wwwroot/images/loader/lg.comet-spinner.gif') !important;*/
    }

    #tbl_ActivationInterval_paginate {
        text-align: center;
        width: 100%;
        padding-top: 10px;
        padding-bottom: 10px;
    }


    .paginate_button {
        width: 50px;
        height: 30px;
        text-align: center;
        font-size: 18px;
        padding-left: 7px;
        padding-right: 7px;
        background-color: white;
        border-color: #a7a9ac;
        padding-top: 10px;
        padding-bottom: 100px;
    }

    #tbl_ActivationInterval_previous {
        width: 80px;
        padding-right: 80px;
        background-color: #d1d3d4;
    }

    #tbl_ActivationInterval_next {
        width: 80px;
        background-color: #d1d3d4;
    }

    .current {
        background: #5c7988 !important;
    }

</style>

<script src="~/js/Special Case/ActivationInterval.js"></script>

<script type="text/javascript" src="~/js/DataTable/DataTable_1_10_19.js"></script>


<script src="~/js/DataTable/Datatables_button_1_5_6.js"></script>
<script src="~/js/DataTable/buttons_flash_1_5_6.js"></script>
<script src="~/js/DataTable/ajax_jszip_3_1_3.js"></script>
<script src="~/js/DataTable/ajax_0_1_53_pdfMake.js"></script>
<script src="~/js/DataTable/pdfMake_0_1_53_vfsFonts.js"></script>
<script src="~/js/DataTable/buttons_1_5_6_html5_buttons.js"></script>
<script src="~/js/DataTable/buttons_1_5_6_print.js"></script>
<script src="~/js/jquery_delete_confirmation/bootstrap-confirm-delete.js"></script>
<h2 style="padding-top:50px"></h2>

<div id="load"></div>

<div class="row" style="padding-top:30px">
    <div class="col-md-8 col-md-offset-2">
        <div class="alldiv">
            <div class="altblhead">
                <h2 class="hdrRobiAxia"> Activation Interval </h2>
            </div>
            <div class="row" style="padding-top:30px;padding-bottom:30px;">
                <div class="col-md-2 col-md-offset-1 dvRobiMedium">Activation Interval <br />(National ID) </div>
                <div class="col-md-5">

                    <input type="number" class="form-control txtNID txtboxdesign inputFont" id="txtTimeLimit">

                </div>
                <div class="col-md-3">
                    <button type="button" class="btn btn-block btnscActInterv btn-dark txtboxdesign btnCommonAll" style="color:white;">Add</button>
                </div>
                <div class="col-md-1">

                </div>
            </div>
        </div>
        <div style="padding-top:20px;">
        </div>
        <div class="alldiv">
            <div class="altblhead">
                <h2 class="hdrRobiAxia">Existing List</h2>
            </div>
            <table class="table" id="tbl_ActivationInterval">
                <thead>
                    <tr>
                        <th class="dvRobiMedium" style="width:70px;text-align:center">#</th>
                        <th class="dvRobiMedium" scope="col" style="text-align:center;">National ID</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody id="tbl_ActivationInterval_List"></tbody>
            </table>
        </div>

    </div>

    <div class="col-md-8 col-md-offset-2">

    </div>


</div>


<script>
    document.onreadystatechange = function () {

        var state = document.readyState
        if (state == 'complete') {
            setTimeout(function () {
                document.getElementById('interactive');
                document.getElementById('load').style.visibility = "hidden";
            }, 1500);
        }
    }
</script>