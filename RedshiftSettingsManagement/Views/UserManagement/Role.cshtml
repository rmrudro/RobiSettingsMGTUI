<h2 style="padding-top:40px"></h2>

<div id="load"></div>

<div class="row" style="padding-top:30px">
    <div class="col-md-8 col-md-offset-2">
        <div class="alldiv">
            <div class="altblhead">
                <h2 class="hdrRobiAxia">
                    Role Information
                    <button type="button" onclick="location.href='./RoleAdd'" class="btn btn-dark channeladd" style="float:right;color:white;">Add</button>
                </h2>
            </div>

            <table id="tblRoletable" class="col-md-12 tblcustom">
                <thead>
                    <tr>
                        <th style="width:10%;text-align:center">#</th>
                        <th style="width:50%;text-align:center" class="dvRobiMedium">Role Name</th>

                        <th style="width:40%;text-align:center" class="dvRobiMedium">Status</th>
                    </tr>

                </thead>
                <tbody id="tbl_allRoleList"></tbody>
            </table>
        </div>
    </div>
    <div class="col-md-2">

    </div>
</div>

<script>

    document.onreadystatechange = function () {

        var state = document.readyState
        if (state == 'complete') {
            setTimeout(function () {
                document.getElementById('interactive');
                document.getElementById('load').style.visibility = "hidden";
            }, 1500);
        }
    }

</script>

